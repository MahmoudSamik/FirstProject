<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>Template | @ViewBag.title </title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    @*        Core CSS          *@  
    <link href="~/Content/bootstrap/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/KendoUI/kendo.common.min.css" rel="stylesheet" />
    <link href="~/Content/KendoUI/kendo.common-bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/KendoUI/kendo.bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/KendoUI/kendo.dataviz.min.css" rel="stylesheet" />
    <link href="~/Content/KendoUI/kendo.dataviz.bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/FontAwesome/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/ColorScheme.css" rel="stylesheet" />
    <link href="~/Content/Styles.css" rel="stylesheet" />
    <link href="~/Content/FontAwesomeKindoIcons.css" rel="stylesheet"/>
    @*          Custome Page Specific CSS             *@
    @{
        var Theme = System.Configuration.ConfigurationManager.AppSettings["Theme"];

        if (Theme == "Theme1")
        {
            
            <link href="~/Content/ThemeOneDesign.css" rel="stylesheet"/>
            <link href="~/Content/ThemeOneColorScheme.css" rel="stylesheet" />
        }
        if (Theme == "Theme2")
        {
            <link href="~/Content/ThemeOneDesign.css" rel="stylesheet" />
            <link href="~/Content/ThemeOneColorScheme.css" rel="stylesheet" />

        }
}
    @RenderSection("HeaderCss", required: false)
</head>
